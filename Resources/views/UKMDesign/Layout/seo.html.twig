<link rel="canonical" href="{{UKMDesign.getHeader().getSeo().canonical}}" />

<meta name="description" content="{{ UKMDesign.getHeader().getSeo().getDescription() }}">
<meta name="author" content="{{ UKMDesign.getHeader().getSeo().getAuthor() }}">
<meta property="og:locale" content="nb_NO" />
<meta property="fb:app_id" content="{{ UKMDesign.getHeader().getSeo().getFBAppId() }}" />
<meta property="fb:admins" content="{{ UKMDesign.getHeader().getSeo().getFBAdmins() }}" />
<meta name="google-site-verification" content="{{ UKMDesign.getHeader().getSeo().getGoogleSiteVerification() }}" />
<meta property="og:site_name" content="{{UKMDesign.getHeader().getSeo().getSiteName()}}" />
<meta property="og:type" content="{{UKMDesign.getHeader().getSeo().getType()}}" />
<meta property="og:title" content="{{UKMDesign.getHeader().getSeo().getTitle()|raw}}"/>
<meta property="article:section" content="{{UKMDesign.getHeader().getSeo().getSection()}}" />
{% if UKMDesign.getHeader().getSeo().getImage().getWidth() > 0 and UKMDesign.getHeader().getSeo().getImage().getHeight() > 0 %}
<meta property="og:image" content="{{UKMDesign.getHeader().getSeo().getImage().getUrl()}}" />
<meta property="og:image:width" content="{{UKMDesign.getHeader().getSeo().getImage().getWidth()}}" />
<meta property="og:image:height" content="{{UKMDesign.getHeader().getSeo().getImage().getHeight()}}" />
{% elseif UKMDesign.getHeader().getSeo().getImage() is not empty %}
<meta property="og:image" content="{{UKMDesign.getHeader().getSeo().getImage().getUrl()}}" />
{% endif %}
{% if UKMDesign.getHeader().getSeo().getImage().getType() is not empty %}
<meta property="og:image:type" content="{{UKMDesign.getHeader().getSeo().getImage().getType()}}" />
{% endif %}
<meta property="og:url" content="{{UKMDesign.getHeader().getSeo().getCanonical()}}" />
<meta property="og:description" content="{{UKMDesign.getHeader().getSeo().getDescription()}}" />
<meta property="og:locale" content="nb_NO" />
{% if UKMDesign.getHeader().getSeo().getType() == 'article' %}
<meta property="article:published_time" content="{{UKMDesign.getHeader().getSeo().getPublished()}}" />
<meta property="article:modified_time" content="{{UKMDesign.getHeader().getSeo().getPublished()}}" />
<meta property="article:author" content="{{UKMDesign.getHeader().getSeo().getAuthor()}}" />
{% endif %}
{% if UKMDesign.getHeader().getSeo().getOembed() != false %}
	<link rel="alternate" type="application/json+oembed" href="{{ UKMDesign.getHeader().getSeo().getOembed() }}" title="{{ UKMDesign.getHeader().getSeo().getTitle()|raw }}" />
{% endif %}