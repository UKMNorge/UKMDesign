			{% set array = ['ukm','ukmtv','vibakukm'] %}
			{% for name in array %}
				{% set section = UKMDesign.getSitemap().getSection(name) %}
				{% set menuTitle = section.getTitle() %}
				<div class="subpages subpage-{{ name }}" id="subpage-{{ name }}">
					<div class="backToMain dropdown-header drop-item" id="backToMain">
						<img src="//assets.{{ UKM_HOSTNAME }}/img/hamburger/chevron-small.svg"/>
						<span>{{ menuTitle }}</span>
					</div>
					<div class="dropdown-divider"></div>
					{% for page in section.getPages().getAll() %}
						{% set title = {'title': page.getTitle(), 'link': page.getUrl(), 'id': name, 'svg': page.getSvg()} %}
						{% include "UKMDesign/Navbar/navbarLine.html.twig" with title %}
					{% endfor %}
				</div>
			{% endfor %}

			<div class="dropdown-divider"></div>
			<div style="display: flex; justify-content: center; padding-bottom: 1.5rem; border-radius: 15px; background: white;">
				{% if app.request.getSchemeAndHttpHost() != 'https://delta.ukm.dev' and app.request.getSchemeAndHttpHost() != 'https://delta.ukm.no' and app.request.getSchemeAndHttpHost() != 'https://delta.ukmdev.no' %}
					{% include "Norge/Front/Components/Standard-button.html.twig" with {title: 'Start påmelding', path: '/fylker/', extra: 'btn-small btn-blue mt-3'} %}
				{% endif %}
			</div>
			<a class="dropdown-header" href="{{ UKMDesign.getSitemap().getPage('UKM','login') }}" style="display: flex; align-items: center; padding: 0.5rem 2.5rem;">
